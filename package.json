{
  "name": "dailycat.api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "copy-public": "ts-node src/utils/copy-public.ts",
    "copy-views": "ts-node src/utils/copy-views.ts",
    "start": "node dist/bin/www.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./dist/bin/www.js\"",
    "prettier": "npx prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "axios": "^1.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "moment": "^2.29.4",
    "mongoose": "^6.7.3",
    "morgan": "~1.9.1",
    "node-schedule": "^2.1.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.8",
    "@types/morgan": "^1.9.4",
    "@types/node-schedule": "^2.1.0",
    "@types/shelljs": "^0.8.12",
    "concurrently": "^8.2.0",
    "del": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "nodemon": "^2.0.22",
    "prettier": "2.8.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
